public class fox{
    void sum1(int num1,int num2)
    {
        System.out.println(num1+num2);
        System.out.println("printed");
         System.out.println(num1-num2);
        System.out.println("printed");
         System.out.println(num1*num2);
        System.out.println("printed");
         System.out.println(num1/num2);
        System.out.println("printed");
    }
    public static void main(String[]args){
        fox obj1=new fox();
        obj1.sum1(2,10);
    }
    
}
Output:
12
printed
-8
printed
20
printed
0
printed

