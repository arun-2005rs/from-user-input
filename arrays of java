import java.lang.System;
import java.util.Scanner;
class main{
    public static void main(String[]args){
    System.out.println("Size of an array");
    Scanner scan=new Scanner(System.in);
    int size=scan.nextInt();
    int[] a = new int[size];
    for(int i=0;i<=size-1;i++){
        a[i]=scan.nextInt();
    }
           }  
    }
