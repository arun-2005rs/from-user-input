class Animal {
    public void eat() {
        System.out.println("This Animal Can Eat");
    }
}

class Dog extends Animal {
    public void bark() {
        System.out.println("Barks");
    }
}
class Jungle {
    public static void main(String[] args) {
        Dog d1 = new Dog();
        d1.eat();
        d1.bark();
    }
}
