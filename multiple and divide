import java.lang.System;
 import java.util.Scanner;
 class main{
     public static void main (String args[]){
         Scanner jo=new Scanner(System.in);
         int a=jo.nextInt();
         int b=jo.nextInt(); 
         int c=jo.nextInt();
         int d=a*b*c;
         int e=a+b+c;
         System.out.println(d/e);
     }
 }
